{
   "name": "devzen",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
      "type-check": "tsc --pretty --noEmit"
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   },
   "lint-staged": {
      "**/*.{js,jsx,ts,tsx,json}": [
         "npm run lint . --fix",
         "prettier --write ."
      ]
   },
   "dependencies": {
      "@tanstack/react-query": "^4.35.3",
      "@types/lint-staged": "^13.2.0",
      "@types/node": "20.4.2",
      "@types/react": "18.2.15",
      "@types/react-dom": "18.2.7",
      "autoprefixer": "10.4.14",
      "mongoose": "^7.4.3",
      "next": "13.4.10",
      "openai": "^3.3.0",
      "postcss": "8.4.26",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-icons": "^4.10.1",
      "tailwindcss": "3.3.3",
      "typescript": "5.1.6"
   },
   "devDependencies": {
      "@tanstack/eslint-plugin-query": "^4.34.1",
      "@typescript-eslint/eslint-plugin": "^6.6.0",
      "@typescript-eslint/parser": "^6.6.0",
      "eslint": "8.48.0",
      "eslint-config-next": "13.4.19",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.0",
      "husky": "^8.0.3",
      "lint-staged": "^14.0.1",
      "prettier": "^3.0.3"
   }
}
