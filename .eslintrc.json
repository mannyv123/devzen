{
   "extends": [
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "plugin:@tanstack/eslint-plugin-query/recommended"
   ],
   "plugins": ["@typescript-eslint", "prettier", "@tanstack/query"],
   "root": true,
   "rules": {
      "no-console": "warn",
      "no-use-before-define": ["error", { "functions": false }]
   }
}
